-- Insert Chain_Control_Categories_Washington
INSERT INTO Chain_Control_Categories_Washington (wa_chain_control_level_description, wa_chain_control_level)
    VALUES
        ("Traction tires are not required but are recommended. Oversize loads prohibited. Oversize vehicles may be restricted from roadways during severe weather conditions.", "Traction Tires Advised"),
        ("Passenger vehicles must use approved traction tires. Chains are required on all vehicles over 10,000 gross vehicle weight (GVW), and all vehicles and loads over 10,000 pounds gross vehicle weight rating (GVWR), including large passenger trucks and SUVs.", "Traction Tires Required"),
        ("Tire chains required - Vehicles must install chains to continue traveling on the roadway. 4WD/AWD vehicles do not need to install chains at this point, but still must carry a set inside their vehicle in case conditions worsen.* See WAC 204-24-050.", "Tire Chains Required"),
        ("Chains required on ALL vehicles, even 4WD/AWD. This is the last step before a pass is closed.", "Chains Required on All Vehicles");


-- Insert Chain_Control_Categories_Calilfornia
INSERT INTO Chain_Control_Categories_California (ca_chain_control_level_description, ca_chain_control_level)
    VALUES
        ("Chains are required on all vehicles except passenger vehicles and light-duty trucks under 6,000 pounds gross weight and equipped with snow tires on at least two drive wheels. Chains must be carried by vehicles using snow tires. All vehicles towing trailers must have chains on one drive axle. Trailers with brakes must have chains on at least one axle.", "Requirement 1 (R-1)"),
        ("Chains or traction devices are required on all vehicles except four-wheel/all-wheel drive vehicles with snow-tread tires on all four wheels. NOTE: (Four-wheel/all-wheel drive vehicles must carry traction devices in chain control areas.)", "Requirement 2 (R-2)"),
        ("Chains or traction devices are required on all vehicles, no exceptions.", "Requirement 3 (R-3)");

